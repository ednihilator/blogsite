<!-- Loop over each object in the projects array -->
<h1>Posts</h1>
{{#each posts as |post| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    
    <p>
      
    <p>{{post.user.name}} on {{format_date post.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      <a href="/post/{{post.id}}">{{post.title}}</a>
      
    </p>
  </div>
</div>
{{/each}}